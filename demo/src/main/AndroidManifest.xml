<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.jp.infinite.indicator.demo">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".DemoListActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".DemoCircleInfiniteIndicatorAct"
            android:label="@string/label_demo_circle_indicator">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="com.jp.infinite.indicator.demo.ACT"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".DemoTitleInfiniteIndicatorAct"
            android:label="@string/label_demo_title_indicator">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="com.jp.infinite.indicator.demo.ACT"/>
            </intent-filter>
        </activity>

    </application>

</manifest>
